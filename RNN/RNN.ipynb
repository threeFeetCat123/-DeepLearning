        "import copy\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "#随机种子，固定的话每个人得到的结果都一致\n",
        "np.random.seed(42)\n",
        "\n",
        "# sigmoid函数\n",
        "def sigmoid(x):\n",
        "    output = 1 / (1 + np.exp(-x))\n",
        "    return output\n",
        "\n",
        "# sigmoid导数\n",
        "def sigmoid_output_to_derivative(output):\n",
        "    return output * (1 - output)\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "# 训练数据生成\n",
        "int2binary = {}\n",
        "binary_dim = 16  # 16位二进制数\n",
        "\n",
        "largest_number = 1 << binary_dim  # 2的16次方，共65536个数\n",
        "\n",
        "def int_to_binary(number, bits=16):\n",
        "    return [int(bit) for bit in format(number, f'0{bits}b')]\n",
        "\n",
        "# 生成所有16位数的二进制表示\n",
        "for i in range(largest_number):\n",
        "    int2binary[i] = np.array(int_to_binary(i), dtype=np.uint8)"
      ]
    },
    {
